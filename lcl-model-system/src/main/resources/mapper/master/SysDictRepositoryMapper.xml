<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.micro.lcl.system.repository.SysDictRepository">
    <select id="dupliateCheckCountSql" parameterType="com.micro.lcl.system.model.DuplicateCheckVo" resultType="int">
        select count(*) from ${tableName} where ${fieldName} = #{fieldVal}
            <if test="dataId !=null "> and id &lt;&gt; #{dataId}</if>
    </select>
</mapper>